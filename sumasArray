public class SumarElementosArray {
    public static void main(String[] args) {
        int[] numeros = {2, 4, 6, 8, 10};
        int suma = 0;
        
        for (int numero : numeros) {
            suma += numero;
        }
        
        System.out.println("La suma de los elementos del array es: " + suma);
    }
}
